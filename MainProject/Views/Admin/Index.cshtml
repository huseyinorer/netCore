@model List<AppIdentityUser>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}


<table class="table table-bordered table-striped ">
    <tr>
        <td>User Id</td>
        <td>User name</td>
        <td>Email</td>
    </tr>
    @if (Model.Count() == 0)
    {
        <tr>
            <td colspan="3">Hiç üye yok!</td>
        </tr>

    }
    else
    {
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.UserName</td>
                <td>@item.Email</td>
            </tr>
        }
    }
</table>

